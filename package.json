{
  "name": "ember-tools",
  "version": "0.0.4",
  "bin": {
    "ember": "./bin/ember",
    "ember-generate": "./bin/ember-generate",
    "ember-precompile": "./bin/ember-precompile"
  },
  "dependencies": {
    "commander": "1.1.1",
    "cli-color": "0.2.1",
    "walk": "2.2.1",
    "handlebars": "1.0.8",
    "jsdom": "0.3.4",
    "fleck": "0.5.1",
    "rsvp-that-works": "*",
    "browserbuild": "0.6.0",
    "fs-extra": "0.5.0"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*",
    "rimraf": "*"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha --require should --reporter dot --ui bdd --growl test",
    "watch-test": "node_modules/.bin/mocha --require should --reporter dot --ui bdd --growl test --watch ./src"
  },
  "engines": { "node" : ">=0.8.16" }
}
